
@model List<Student>
@{
    int count = 0;
}

<table class="table">
    <thead>
        <tr>
            <td>No</td>
            <td>Name</td>
            <td>Surname</td>
            <td>Age</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(++count)</td>
                <td>@(item.Name)</td>
                <td>@(item.Surname)</td>
                <td>@(item.Age)</td>
            </tr>
        }
    </tbody>
</table>


